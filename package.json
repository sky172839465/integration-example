{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "clean": "rm -rf build coverage reports",
    "analyze": "npm run build && source-map-explorer",
    "\\n# LINTER SCRIPTS": "",
    "prepare": "husky install",
    "lint": "eslint .",
    "lintfix": "npm run lint -- --fix",
    "style": "stylelint **/*.css",
    "stylefix": "npm run style -- --fix",
    "\\n# UNIT TEST SCRIPTS": "",
    "pre-test": "npm i @~/src@file:src && cross-env CI=true",
    "test:base": "npm run pre-test -- react-scripts test --roots=__tests__",
    "test": "npm run test:base -- --color --coverage --coverageReporters=text",
    "test:report": "npm run test -- --testResultsProcessor=jest-junit",
    "\\n# FUNCTIONAL TEST SCRIPTS": "",
    "pre-functional": "cross-env CI=true JEST_JUNIT_OUTPUT_NAME=functional-test.xml",
    "functional": "npm run pre-functional -- jest -c __e2e__/jest.config.js --color",
    "\\n# CI SCRIPTS": "",
    "ci-test": "npm run test:report; node ./scripts/lint",
    "ci-functional": "npm run functional; node ./scripts/functional"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@~/src": "file:src",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/expect-puppeteer": "file:^4.4.5 @types/jest-environment-puppeteer@^4.4.1",
    "@types/puppeteer": "^5.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "jest-junit": "^12.0.0",
    "jest-puppeteer": "^5.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "xml2js": "^0.4.23"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run lintfix"
    ],
    "**/*.{json,md,yml}": [
      "prettier --write"
    ],
    "**/*.{css,scss}": [
      "npm run stylefix"
    ]
  },
  "jest": {
    "testMatch": [
      "**/*.test.js"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./reports",
    "outputName": "unit-test.xml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
