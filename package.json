{
  "name": "example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && source-map-explorer",
    "\\n# LINTER SCRIPTS": "",
    "prepare": "husky install",
    "lint": "eslint .",
    "lintfix": "npm run lint -- --fix",
    "style": "stylelint **/*.css",
    "stylefix": "npm run style -- --fix"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run lintfix"
    ],
    "**/*.{json,md,yml}": [
      "prettier --write"
    ],
    "**/*.{css,scss}": [
      "npm run stylefix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
